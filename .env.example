# WebSocket Server Settings
WEBSOCKET_HOST=0.0.0.0
WEBSOCKET_PORT=8080
WEBSOCKET_SSL=false
WEBSOCKET_SSL_CERT=
WEBSOCKET_SSL_KEY=
WEBSOCKET_SSL_VERIFY=false

# Performance Settings
WEBSOCKET_MAX_CLIENTS=1000
WEBSOCKET_MAX_CONN_PER_IP=10
WEBSOCKET_MEMORY_LIMIT=128M
WEBSOCKET_BUFFER_SIZE=8192

# Connection Settings
WEBSOCKET_PING_INTERVAL=30
WEBSOCKET_PING_TIMEOUT=60
WEBSOCKET_DISCONNECT_TIMEOUT=120
WEBSOCKET_THROTTLE=false
WEBSOCKET_THROTTLE_MAX=100
WEBSOCKET_THROTTLE_PER=60

# Security Settings
WEBSOCKET_ALLOWED_ORIGINS=*
WEBSOCKET_ALLOWED_IPS=*
WEBSOCKET_BLOCKED_IPS=
WEBSOCKET_MAX_MESSAGE_SIZE=65536
WEBSOCKET_RATE_LIMIT=false
WEBSOCKET_RATE_MAX=100
WEBSOCKET_RATE_WINDOW=60

# Logging Settings
WEBSOCKET_LOGGING=true
WEBSOCKET_LOG_LEVEL=info
WEBSOCKET_LOG_FILE=storage/logs/websocket.log
WEBSOCKET_LOG_SEPARATE=false
WEBSOCKET_LOG_MAX_FILES=5
WEBSOCKET_LOG_CONNECTIONS=true
WEBSOCKET_LOG_MESSAGES=false

# Event Settings
WEBSOCKET_QUEUE_EVENTS=false
WEBSOCKET_ASYNC_EVENTS=true

# Debug Settings
WEBSOCKET_DEBUG=false
WEBSOCKET_DEBUG_VERBOSE=false
WEBSOCKET_DEBUG_FRAMES=false
WEBSOCKET_DEBUG_MEMORY=false
